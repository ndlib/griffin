<%= simple_form_for @item,
  :url => url_for(:action => 'create', :controller => 'admin/video') do |v| %>
  <%= v.error_notification %>
  <%= v.input :title %>
  <%= v.input :url, :label => "Video Link" %>
  <%= v.input :edition %>
  <%= v.input :publisher %>
  <%= v.input :display_note %>
  <%= v.button :submit %>
<% end %>
