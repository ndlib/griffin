<div class="container">
  <div class="row">
    <div class="span9">
      <h1>Video Items</h1>
      <div class="span12">
        <table class="table table-bordered admin_data_table">
              <thead>
                <th>System ID</th>
                <th>Name</th>
                <th>Description</th>
              </thead>
              <tbody>
                <% for video in @videos %>
                <tr>
                  <td>
                    <%= link_to video[:id], edit_admin_video_path(video) %>
                  </td>
                  <td>
                    <%= video[:name] %>
                  </td>
                  <td>
                    <%= video[:description] %>
                  </td>
                </tr>
                <% end %>
              </tbody>
          </table>
      </div>
    </div>
  </div>
</div>
