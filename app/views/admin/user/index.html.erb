<div class="container">
  <div class="row">
    <div class="span9">
      <h1>User Administration</h1>
      <div class="span12">
        <table class="table table-bordered admin_data_table">
              <thead>
                <th>System ID</th>
                <th>Last Name</th>
                <th>First Name</th>
                <th>User ID</th>
              </thead>
              <tbody>
                <% for user in @users %>
                <tr>
                  <td>
                    <%= link_to user[:id], admin_user_path(user) %>
                  </td>
                  <td>
                    <%= user[:last_name] %>
                  </td>
                  <td>
                    <%= user[:first_name] %>
                  </td>
                  <td>
                    <%= user[:username] %>
                  </td>
                </tr>
                <% end %>
              </tbody>
          </table>
      </div>
    </div>
  </div>
</div>
