<% object = AdminReserveRow.new(object) %>
<tr>
  <td>
    <%= render partial: '/requests/admin_row_button', locals: { button: object.button } %>
  </td>
  <td><%= object.request_date %></td>
  <td><%= object.needed_by %></td>
  <td><%= object.title %></td>
  <td><%= link_to object.requestor, new_masquerades_path(:username => object.requestor_netid) %></td>
  <td><%= object.course_title %></td>
  <td><%= object.type %></td>
  <td><%= raw object.subtitles %></td>
  <td>
    <%= object.missing_data %>
  </td>
</tr>

